   "source": [
    "import cv2\n",
    "VidCap=cv2.VideoCapture(0)\n",
    "while(True):\n",
    "    ret,frame=VidCap.read()\n",
    "    cv2.imshow('video_image',frame)\n",
    "    if cv2.waitKey(1) == ord('q'):\n",
    "        break\n",
    "VidCap.release()\n",
    "cv2.destroyAllWindows()"
   ]
